{
  "name": "postcss-color",
  "version": "1.0.1",
  "description": "PostCSS plugin to transform latest W3C CSS color module syntax to more compatible CSS",
  "keywords": [
    "css",
    "postcss",
    "postcss-plugins",
    "color",
    "colour",
    "rgb",
    "hwb",
    "rebeccapurple",
    "color-function",
    "function"
  ],
  "author": "MoOx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/postcss/postcss-color.git"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "lib",
    "index.js"
  ],
  "dependencies": {
    "balanced-match": "^0.1.0",
    "color": "^0.7.1",
    "css-color-function": "^1.1.1",
    "reduce-function-call": "^1.0.1"
  },
  "devDependencies": {
    "jscs": "^1.5.9",
    "jshint": "^2.5.2",
    "jshint-stylish": "^0.4.0",
    "postcss": "^2.1.0",
    "tap-colorize": "^1.2.0",
    "tape": "^2.13.4"
  },
  "scripts": {
    "jscs": "jscs *.js **/*.js",
    "jshint": "jshint . --exclude node_modules --reporter node_modules/jshint-stylish/stylish.js",
    "test": "npm run jscs && npm run jshint && tape test | tap-colorize"
  }
}
